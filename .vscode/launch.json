{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Fuznet",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/fuznet",
      "args": ["-c", "config/settings.toml", "-l", "hardware/cells/xilinx.yaml"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
